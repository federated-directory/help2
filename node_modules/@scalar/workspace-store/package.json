{
  "name": "@scalar/workspace-store",
  "description": "Store interface for openapi documents",
  "license": "MIT",
  "author": "Scalar (https://github.com/scalar)",
  "homepage": "https://github.com/scalar/scalar",
  "bugs": "https://github.com/scalar/scalar/issues/new/choose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalar/scalar.git",
    "directory": "packages/workspace-store"
  },
  "keywords": [
    "workspace",
    "store",
    "openapi",
    "scalar"
  ],
  "version": "0.28.1",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "exports": {
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./events": {
      "import": "./dist/events/index.js",
      "types": "./dist/events/index.d.ts",
      "default": "./dist/events/index.js"
    },
    "./helpers/*": {
      "import": "./dist/helpers/*.js",
      "types": "./dist/helpers/*.d.ts",
      "default": "./dist/helpers/*.js"
    },
    "./mutators": {
      "import": "./dist/mutators/index.js",
      "types": "./dist/mutators/index.d.ts",
      "default": "./dist/mutators/index.js"
    },
    "./navigation": {
      "import": "./dist/navigation/index.js",
      "types": "./dist/navigation/index.d.ts",
      "default": "./dist/navigation/index.js"
    },
    "./persistence": {
      "import": "./dist/persistence/index.js",
      "types": "./dist/persistence/index.d.ts",
      "default": "./dist/persistence/index.js"
    },
    "./plugins/bundler": {
      "import": "./dist/plugins/bundler/index.js",
      "types": "./dist/plugins/bundler/index.d.ts",
      "default": "./dist/plugins/bundler/index.js"
    },
    "./plugins/client": {
      "import": "./dist/plugins/client/index.js",
      "types": "./dist/plugins/client/index.d.ts",
      "default": "./dist/plugins/client/index.js"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts",
      "default": "./dist/schemas.js"
    },
    "./schemas/*": {
      "import": "./dist/schemas/*.js",
      "types": "./dist/schemas/*.d.ts",
      "default": "./dist/schemas/*.js"
    },
    "./schemas/extensions/operation": {
      "import": "./dist/schemas/extensions/operation/index.js",
      "types": "./dist/schemas/extensions/operation/index.d.ts",
      "default": "./dist/schemas/extensions/operation/index.js"
    },
    "./schemas/extensions/workspace": {
      "import": "./dist/schemas/extensions/workspace/index.js",
      "types": "./dist/schemas/extensions/workspace/index.d.ts",
      "default": "./dist/schemas/extensions/workspace/index.js"
    },
    "./schemas/v3.1/strict/openapi-document": {
      "import": "./dist/schemas/v3.1/strict/openapi-document.js",
      "types": "./dist/schemas/v3.1/strict/openapi-document.d.ts",
      "default": "./dist/schemas/v3.1/strict/openapi-document.js"
    },
    "./schemas/v3.1/strict/type-guards": {
      "import": "./dist/schemas/v3.1/strict/type-guards.js",
      "types": "./dist/schemas/v3.1/strict/type-guards.d.ts",
      "default": "./dist/schemas/v3.1/strict/type-guards.js"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./workspace-plugin": {
      "import": "./dist/workspace-plugin.js",
      "types": "./dist/workspace-plugin.d.ts",
      "default": "./dist/workspace-plugin.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@scalar/typebox": "0.1.3",
    "github-slugger": "^2.0.0",
    "type-fest": "^5.3.1",
    "vue": "^3.5.26",
    "yaml": "^2.8.0",
    "@scalar/code-highlight": "0.2.2",
    "@scalar/json-magic": "0.9.5",
    "@scalar/helpers": "0.2.10",
    "@scalar/object-utils": "1.2.24",
    "@scalar/openapi-upgrader": "0.1.8",
    "@scalar/snippetz": "0.6.10",
    "@scalar/themes": "0.14.0",
    "@scalar/types": "0.6.1"
  },
  "devDependencies": {
    "@google-cloud/storage": "7.16.0",
    "fake-indexeddb": "6.2.3",
    "fastify": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "4.0.16",
    "@scalar/build-tooling": "0.4.1"
  },
  "scripts": {
    "build": "scalar-build-esbuild",
    "format": "scalar-format",
    "format:check": "scalar-format-check",
    "generate-json-schemas": "vite-node ./scripts/generate-json-schemas.ts",
    "lint:check": "scalar-lint-check",
    "lint:fix": "scalar-lint-fix",
    "test": "vitest",
    "types:build": "scalar-types-build",
    "types:check": "scalar-types-check"
  }
}